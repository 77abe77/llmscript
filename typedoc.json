{
  "$schema": "https://typedoc.org/schema.json",
  "plugin": [
    "typedoc-plugin-markdown",
    "typedoc-plugin-frontmatter",
    "./custom-frontmatter.mjs"
  ],
  "pretty": true,
  "mergeReadme": true,
  "cleanOutputDir": true,
  "hidePageHeader": true,
  "hideBreadcrumbs": true,
  "useHTMLAnchors": true,
  "preserveAnchorCasing": false,
  "readme": "README.md"
}
